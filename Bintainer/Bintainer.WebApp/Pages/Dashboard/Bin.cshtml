@page "/dashboard/bin"
@model BinModel
@{
}

<meta name="viewport" content="width=device-width, initial-scale=1.0">
<link rel="stylesheet" href="~/css/autocomplete.css">


<h2>Autocomplete</h2>

<p>Start typing:</p>

<!--Make sure the form has the autocomplete function switched off:-->
<form autocomplete="off" action="/action_page.php">
    <div class="col-md-4">
        <div class="autocomplete" style="width:300px;">
            <input type="text" class="form-control input-sharp" id="searchPartName" placeholder="Part Name" data-autocomplete-options="partNamesData">
            <div id="partNamesData" style="display:none;">
                @Html.Raw(Json.Serialize(Model.Part.Select(p => p.Number.Trim())))
            </div>
        </div>        
    </div>
    <input type="submit">
</form>